<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <HBox prefHeight="40.0" prefWidth="400.0">
        <Label>Test</Label>
    </HBox>
    <HBox prefHeight="260.0" prefWidth="400.0">
        <GridPane fx:id="NumbersPane">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" fillWidth="true" />
                <ColumnConstraints hgrow="ALWAYS" fillWidth="true" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="ALWAYS" fillHeight="true" />
                <RowConstraints vgrow="ALWAYS" fillHeight="true" />
                <RowConstraints vgrow="ALWAYS" fillHeight="true" />
                <RowConstraints vgrow="ALWAYS" fillHeight="true" />
            </rowConstraints>
        </GridPane>
        <GridPane alignment="TOP_RIGHT">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
            </rowConstraints>
        </GridPane>
    </HBox>
</VBox>
